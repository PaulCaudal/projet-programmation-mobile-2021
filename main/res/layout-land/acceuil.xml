<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lLayouthisto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF9800"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/acceuil">

    <GridLayout
        android:id="@+id/gridLayout1"
        android:layout_width="match_parent"
        android:layout_height="83dp"
        android:columnCount="3"
        android:rowCount="1"
        app:layout_constraintEnd_toEndOf="parent"
        tools:context=".MainActivity"
        tools:ignore="MissingConstraints"
        tools:showIn="@layout/acceuil">

        <TextView
            android:id="@+id/textViewPlayervs"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="150dp"
            android:layout_row="0"
            android:gravity="center"
            android:layout_column="1"
            android:text="@string/newmatch"
            android:textSize="24sp" />

        <ImageButton
            android:id="@+id/backButton"
            android:layout_width="89dp"
            android:layout_height="46dp"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_marginTop="24dp"
            android:background="#FF9800"
            android:onClick="myClickHandler"
            android:tint="#070707"
            android:visibility="visible"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_menu_revert"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="7dp"
            tools:visibility="visible" />

        <Button
            android:id="@+id/buttondem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="150dp"
            android:onClick="myClickHandler"
            android:text="@string/demarer"
            android:textColor="#000000"
            android:visibility="visible"
            app:backgroundTint="#FF9800"
            tools:visibility="visible" />
    </GridLayout>

    <GridLayout
        android:id="@+id/gridLayout2"
        android:layout_width="600dp"
        android:layout_height="626dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="60dp"
        android:columnCount="4"
        android:rowCount="2"
        app:layout_constraintEnd_toEndOf="parent"
        tools:context=".MainActivity"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="214dp"
        tools:showIn="@layout/acceuil">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="88dp"
            android:layout_height="92dp"
            android:layout_row="0"
            android:layout_column="0"
            android:background="#FF9800"
            android:contentDescription="plyers_icon"
            android:visibility="visible"
            app:srcCompat="@android:drawable/ic_menu_myplaces"

            app:tint="#000000"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="32dp"
            tools:layout_editor_absoluteY="303dp"
            tools:visibility="visible" />


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="88dp"
            android:layout_height="92dp"
            android:layout_row="1"
            android:layout_column="0"
            android:background="#FF9800"
            android:visibility="visible"
            app:srcCompat="@android:drawable/ic_menu_myplaces"
            app:tint="#000000"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="32dp"
            tools:layout_editor_absoluteY="147dp"
            tools:visibility="visible" />


        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="88dp"
            android:layout_height="92dp"
            android:layout_row="0"
            android:layout_column="2"
            android:background="#FF9800"
            android:onClick="myClickHandler"
            android:tint="#040404"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.099"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@android:drawable/ic_menu_my_calendar"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="608dp"
            tools:visibility="visible" />

        <EditText
            android:id="@+id/editTextTextPostalAddress"
            android:layout_width="223dp"
            android:layout_height="46dp"
            android:layout_row="0"
            android:layout_column="3"
            android:background="#FF9800"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPostalAddress"
            android:layout_marginTop="20dp"
            android:textSize="24sp"
            android:visibility="visible"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="141dp"
            tools:layout_editor_absoluteY="483dp"
            tools:text="@string/addex"
            tools:visibility="visible" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="223dp"
            android:layout_height="50dp"
            android:layout_row="1"
            android:layout_column="1"
            android:background="#FF9800"
            android:ems="10"
            android:gravity="center"
            android:layout_marginTop="20dp"
            android:inputType="textPersonName"
            android:text="@string/j2"
            android:textSize="24sp"
            android:visibility="visible"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="141dp"
            tools:layout_editor_absoluteY="325dp"
            tools:visibility="visible" />

        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="219dp"
            android:layout_marginTop="20dp"
            android:layout_height="53dp"
            android:layout_row="0"
            android:layout_column="1"
            android:background="#FF9800"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:text="@string/j1"
            android:textColor="#121212"
            android:textSize="24sp"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.755"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="169dp"
            tools:visibility="visible" />
    </GridLayout>
</LinearLayout>